<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	
	<!-- 默认背景色 -->
	<StaticResource x:Key="WindowDefaultBackground" ResourceKey="Primary0Color" />

	<!-- 默认背景回退色 -->
	<StaticResource x:Key="WindowDefaultTransparencyBackgroundFallback" ResourceKey="Primary0Color" />
	
	<!-- 默认前景色 -->
	<StaticResource x:Key="WindowDefaultForeground" ResourceKey="Foreground0Color" />

</ResourceDictionary>