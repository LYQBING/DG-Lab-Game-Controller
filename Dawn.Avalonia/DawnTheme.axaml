<Styles x:Class="Avalonia.DawnTheme.DawnTheme"
		xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Styles.Resources>
		<ResourceDictionary>

			<!-- 字典合并 -->
			<ResourceDictionary.MergedDictionaries>
				<!-- 默认变量 -->
				<ResourceInclude Source="/Controls/Index.axaml" /> <!-- 组件库 -->
				<ResourceInclude Source="/Icons/Index.axaml" /> <!-- 图标库 -->
				<!-- 默认变量 -->
				<ResourceInclude Source="/Variables/Default/Index.axaml" />
			</ResourceDictionary.MergedDictionaries>

			<!-- 主题变体 -->
			<ResourceDictionary.ThemeDictionaries>
				<ResourceInclude x:Key="Default" Source="/Themes/Dark/Index.axaml" />
			</ResourceDictionary.ThemeDictionaries>

		</ResourceDictionary>
	</Styles.Resources>
</Styles>
